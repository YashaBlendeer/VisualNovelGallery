{
  "name": "redux-course-ru-v2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "eslint": "^8.5.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "prepush": "lint-staged",
    "eslint": "node_modules/.bin/eslint src/",
    "lint": "eslint \"**/*.js\"",
    "fix": "eslint --fix \"**/*.js\""
  },
  "devDependencies": {
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "lint-prepush": "^2.2.0",
    "lint-staged": "^11.2.3",
    "prettier": "^1.14.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-prepush"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "node_modules/.bin/eslint --max-warnings=0",
      "eslint --fix",
      "git add"
    ],
    "*.{html,css,sass,scss,less}": [
      "prettier --write",
      "git add"
    ]
  },
  "lint-prepush": {
    "base": "master",
    "tasks": {
      "**/*.{js,jsx}": [
        "eslint"
      ]
    }
  }
}
